{
  "name": "react-comp-pack",
  "version": "1.0.12",
  "description": "A package of react components",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "react",
    "toast",
    "components"
  ],
  "author": "arfi720",
  "license": "ISC",
  "scripts": {
    "prepublish": "run-script-os",
    "prepublish:darwin:linux": "rm -rf ./dist && npm run build",
    "prepublish:win32": "rmdir /s dist && npm run build",
    "build": "webpack --mode development",
    "clean": "run-script-os",
    "clean:darwin:linux": "rm dist",
    "clean:win32": "rmdir /s dist",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "start": "npx react-scripts start",
    "start-server": "webpack serve --hot --mode development"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "run-script-os": "^1.1.6",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
